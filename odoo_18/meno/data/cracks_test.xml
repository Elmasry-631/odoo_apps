<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="ir_cron_scheduler_cracks" model="ir.cron">
			<field name="name">Cracks fast scheduler</field>
			<field name="model_id" ref="base.model_ir_config_parameter" />
			<field name="state">code</field>
			<field name="interval_number">1</field>
			<field name="interval_type">minutes</field>
			<field name="nextcall" eval="(datetime.now() + timedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field eval="True" name="active"/>
			<field name="code">model.cracks()</field>
		</record>
    </data>
</odoo>